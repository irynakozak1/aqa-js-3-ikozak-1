{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "import/extensions": [
            "error",
            "always"
        ]
    }
}